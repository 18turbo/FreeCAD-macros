                NumDig = OldFormat.find("%.")
                if NumDig != -1:
                    NumDig = NumDig+2
                    CharList = list(OldFormat)
                    Digit = int(CharList[NumDig])
                    if Digit < 9:
                        Digit = Digit+1            
                    NewFormat = OldFormat[0:NumDig]+str(Digit)+OldFormat[NumDig+1:]
                    Elem.Object.FormatSpec = NewFormat

