            Direction = DimPoints[0].sub(DimPoints[1])
            Normal = app.Vector(Direction.y,-Direction.x,0)
            CarrierLine = Part.Line(DimPos,DimPos.add(Direction))
            for Dimension in ObliqueDimList:
                DimPos = app.Vector(Dimension.X,Dimension.Y,0)
                NormalLine = Part.Line(DimPos,DimPos.add(Normal))
                Ip = CarrierLine.intersect(NormalLine)
                Dimension.X, Dimension.Y = Ip[0].X,Ip[0].Y

